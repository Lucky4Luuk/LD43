function updatemap()
	h=48
	local cx=flr(camx/8)
	for i=1+cx,128+cx do
		local pd=terrain[max(i-1,1)]
		local d=terrain[i]
		local nd=terrain[min(i+1,1024)]
		local n=1
		if d==0 and pd==0 then
			n=1
		elseif d==-1 and (pd==0 or pd==-1) then
			n=2
		elseif d==1 and (pd==0 or pd==1) then
			n=5
		end
		if d==-1 then
			h-=8
		end
		mset(i-1-cx,h/8,n)
		for y=h/8+1,15 do
			mset(i-1,y,4)
		end
		if i%50==5 then
			if pd==0 and d==0 and nd==0 then
				addobject("campfire",(i-2)*8,h+1*8)
			end
		end
		if n==2 then
			mset(i-1-cx,h/8+1,16)
			if terrainflowers[i-1]!=52 then
				mset(i-1-cx,h/8-1,22)
			else
				mset(i-1-cx,h/8-1,37)
			end
		elseif n==5 then
			mset(i-1-cx,h/8+1,17)
			if terrainflowers[i+1]!=52 then
				mset(i-1-cx,h/8-1,22)
			else
				mset(i-1-cx,h/8-1,35)
			end
		end
		if terrainflowers[i] then mset(i-1-cx,h/8-1,terrainflowers[i]) end
		if terrainflowersfg[i] then mset(i-1-cx,h/8-1+19,terrainflowersfg[i]) end
		if terrainflowers[i]==52 then
			if pd==0 and d==0 and nd==0 and terrainflowers[i-1]!=52 and terrainflowers[i-2]!=52 then
				terrainflowers[i]=52
				mset(i-1-cx,h/8-2,36)
 			mset(i-1-cx,h/8-3,20)
 			mset(i-2-cx,h/8-2,35)
 			mset(i-2-cx,h/8-3,19)
 			mset(i-cx,h/8-2,37)
 			mset(i-cx,h/8-3,21)
			else
				terrainflowers[i]=6
				mset(i-1-cx,h/8-1,6)
			end
		end
		if terrainflowersfg[i]==52 then
			if pd==0 and d==0 and nd==0 and terrainflowersfg[i-1]!=52 and terrainflowersfg[i-2]!=52 then
				terrainflowersfg[i]=52
				mset(i-1-cx,h/8-2+19,36)
 			mset(i-1-cx,h/8-3+19,20)
 			mset(i-2-cx,h/8-2+19,35)
 			mset(i-2-cx,h/8-3+19,19)
 			mset(i-cx,h/8-2+19,37)
 			mset(i-cx,h/8-3+19,21)
			else
				terrainflowersfg[i]=6
				mset(i-1-cx,h/8-1+19,23)
			end
		end
		h+=d*8
		if d==-1 then
			h+=8
		end
		h=min(h/8,16)*8
	end
end

function gen_terrain(seed)
	srand(seed)
	local d=0
	local h=48
	local ph=48
 for i=1,1024 do
		if d==0 then --going straight
			local r=flr(rnd(3))
			if r==2 then
				d=flr(rnd(3)-1) --either -1,0 or -1
			end
		elseif d==-1 then --going up
			local r=flr(rnd(3))
			if r==2 then
				d=0
			end
		elseif d==1 then --going down
			local r=flr(rnd(3))
			if r==2 then
				d=0
			end
		end
		h+=d*8
		if h/8<3 then
			d=0
			h+=8
		end
		if h/8>6 then
			d=0
			h-=8
		end
		terrain[i]=d
		for j=0,7 do
			ph+=d
			terrainh[i*8+j]=ph
		end
		if d==0 then
			--background plants
			local fr=flr(rnd(80))
			if fr<15 then
				--mset(i-1,h/8-1,3)
				terrainflowers[i]=3
			elseif fr<20 then
				terrainflowers[i]=18
			elseif fr<50 then
				terrainflowers[i]=52
			else
				--mset(i-1,h/8-1,6)
				terrainflowers[i]=6
			end
			
			--foreground plants
			fr=flr(rnd(80))
			if fr<15 then
				--mset(i-1,h/8-1,3)
				terrainflowersfg[i]=3
			elseif fr<20 then
				terrainflowersfg[i]=18
			elseif fr<50 then
				terrainflowersfg[i]=52
			else
				--mset(i-1,h/8-1,6)
				terrainflowersfg[i]=23
			end
		end
		--line(i*8,h-d,i*8+8,h,7)
 end
	
	--generate clouds
	for i=1,20 do
		clouds[i]={x=rnd(1024),y=rnd(32)+8}
	end
 
	updatemap()
end

