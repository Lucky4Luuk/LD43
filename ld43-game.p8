function addobject(name,x,y)
	objects[#objects+1]={name=name,x=x,y=y}
	if name=="campfire" then
		local char="default"
		local r=flr(rnd(8))
		if r<3 then
			char="bard"
		elseif r<5 then
			char="lumberjack"
		elseif r<6 then
			char="knight"
		elseif r<7 then
			char="archer"
		elseif r<8 then
			char="priest"
		end
		objects[#objects].char=char
		objects[#objects].char_id=#objects+1
		addobject(char,x+16,y)
	end
end

function _update()
	frame=(frame+1)%32000
	if state=="test" then
		animdata.sword_test.frame+=flr(1/animdata.sword_test.speed)%4
	elseif state=="story_0" then
		updatestory_0()
	elseif state=="dialog" then
		updatedialog()
	elseif state=="game_load" then
		gen_terrain(seed)
		state="story_0"
		substate="text0"
	elseif state=="game" then
		animdata.player.state="idle"
 	if btn(0) then
 		x-=player_speed
 		f=true
 		animdata.player.state="move"
 		--y+=terrain[max(min(flr(x/8+1.5),16),1)]*-1
 	end
 	if btn(1) then
 		x+=player_speed
 		f=false
 		animdata.player.state="move"
 		--y+=terrain[max(min(flr(x/8+1.5),16),1)]
 	end
 	if btn(0) and btn(1) then
 		animdata.player.state="idle"
 	end
 	x=max(0,x)
 	local y1=terrainh[flr(x+19)]+16
 	local y2=terrainh[flr(x+11)]+16
 	y=min(y1,y2)
 	camx=max(0,x-64)
 	camy=min(0,y-52)
 	
 	for i=1,#clouds do
 		clouds[i].x-=cloudspeed
 		if clouds[i].x<-40 then
 			clouds[i].x=1024
 		end
 	end

 	animdata.player.frame=(animdata.player.frame+1/animdata.player.speed)%4
		animdata.campfire.frame=(animdata.campfire.frame+1/animdata.campfire.speed)%8
	end
end

function drawcloud(x,y)
	sspr(88,0,40,16,flr(x)-camx,flr(y)-camy,40,16,false,false)
end

