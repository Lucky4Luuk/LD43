	spr(192,x,y)
	spr(194,x+w*8+8,y)
	spr(224,x,y+h*8+8)
	spr(226,x+w*8+8,y+h*8+8)
	for i=1,(w or 11) do
		spr(193,x+i*8,y)
		spr(225,x+i*8,y+h*8+8)
	end
	for i=1,(h or 11) do
		spr(208,x,y+i*8)
		spr(210,x+w*8+8,y+i*8)
	end
	for i=1,(w or 11) do
		for j=1,(h or 11) do
			spr(209,x+i*8,y+j*8)
		end
	end
	palt(14,false)
end

